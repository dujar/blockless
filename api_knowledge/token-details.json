                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/TokenPriceChangeResponseDto"
                                }
                            }
                        }
                    },
                    "500": {
                        "description": "Fatal server error"
                    }
                }
            },
            "post": {
                "operationId": "PricesController_getTokenListPriceByInterval",
                "summary": "Returns token price change by interval for a list of tokens",
                "parameters": [
                    {
                        "name": "chain",
                        "required": true,
                        "in": "path",
                        "description": "Chain id of network",
                        "example": "1",
                        "schema": {
                            "type": "number"
                        }
                    }
                ],
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/GetTokenListPriceRequestDto"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Token price changes over the given interval",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "array",
                                    "items": {
                                        "$ref": "#/components/schemas/TokenListPriceChangeResponseDto"
                                    }
                                }
                            }
                        }
                    },
                    "400": {
                        "description": "If data format is invalid or there are validation errors"
                    },
                    "404": {
                        "description": "If one or more tokens were not found in the price listings"
                    },
                    "500": {
                        "description": "Fatal server error"
                    }
                }
            }
        },
