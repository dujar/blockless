{
    "openapi": "3.0.0",
    "paths": {
        "/v1.0/details/{chain}": {
            "get": {
                "operationId": "DetailsController_getChainInfo",
                "summary": "Returns details for native token",
                "parameters": [
                    {
                        "name": "chain",
                        "required": true,
                        "in": "path",
                        "description": "Chain id of network",
                        "example": "1",
                        "schema": {
                            "type": "number"
                        }
                    },
                    {
                        "name": "provider",
                        "required": false,
                        "in": "query",
                        "description": "name of chart provider (optional)",
                        "schema": {
                            "enum": [
                                "coinmarketcap",
                                "coingecko"
                            ],
                            "type": "string"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Return token info",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/InfoDataResponse"
                                }
                            }
                        }
                    },
                    "500": {
                        "description": "Fatal server error"
                    }
                }
            }
        },
