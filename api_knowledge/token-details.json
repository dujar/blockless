        "/v1.0/prices/change/{chain}": {
            "get": {
                "operationId": "PricesController_getNativeTokenPriceByInterval",
                "summary": "Returns native token price change by interval",
                "parameters": [
                    {
                        "name": "chain",
                        "required": true,
                        "in": "path",
                        "description": "Chain id of network",
                        "example": "1",
                        "schema": {
                            "type": "number"
                        }
                    },
                    {
                        "name": "interval",
                        "required": true,
                        "in": "query",
                        "description": "eg. 24h,3d,14d,max",
                        "example": "24h",
                        "schema": {
                            "enum": [
                                "5m",
                                "10m",
                                "15m",
                                "30m",
                                "50m",
                                "1h",
                                "2h",
                                "3h",
                                "4h",
                                "6h",
                                "12h",
