    "components": {
        "schemas": {
            "SocialLink": {
                "type": "object",
                "properties": {
                    "name": {
                        "type": "string"
                    },
                    "url": {
                        "type": "string"
                    },
                    "handle": {
                        "type": "string"
                    }
                },
                "required": [
                    "name",
                    "url",
                    "handle"
                ]
            },
            "AssetsResponse": {
                "type": "object",
                "properties": {
                    "name": {
                        "type": "string"
                    },
                    "website": {
                        "type": "string"
                    },
                    "sourceCode": {
                        "type": "string"
                    },
                    "whitePaper": {
                        "type": "string"
                    },
                    "description": {
                        "type": "string"
                    },
                    "shortDescription": {
                        "type": "string"
                    },
                    "research": {
                        "type": "string"
                    },
                    "explorer": {
                        "type": "string"
                    },
                    "social_links": {
                        "$ref": "#/components/schemas/SocialLink"
                    }
                },
                "required": [
                    "name",
                    "website",
                    "sourceCode",
                    "whitePaper",
                    "description",
                    "shortDescription",
                    "research",
                    "explorer",
                    "social_links"
                ]
            },
            "DetailsResponse": {
                "type": "object",
                "properties": {
                    "provider": {
                        "type": "string"
                    },
                    "providerURL": {
                        "type": "string"
                    },
                    "vol24": {
                        "type": "number"
                    },
                    "marketCap": {
                        "type": "number"
                    },
                    "circulatingSupply": {
                        "type": "number"
                    },
                    "totalSupply": {
                        "type": "number"
                    }
                },
                "required": [
                    "provider",
                    "providerURL",
                    "vol24",
                    "marketCap",
                    "circulatingSupply",
                    "totalSupply"
                ]
            },
            "InfoDataResponse": {
                "type": "object",
                "properties": {
                    "assets": {
                        "$ref": "#/components/schemas/AssetsResponse"
                    },
                    "details": {
                        "$ref": "#/components/schemas/DetailsResponse"
                    }
                },
                "required": [
                    "assets",
                    "details"
                ]
            },
            "ChartPointResponse": {
                "type": "object",
                "properties": {
                    "t": {
                        "type": "number",
                        "description": "unix time at sec"
                    },
                    "v": {
                        "type": "number",
                        "description": "price at usd"
                    }
                },
                "required": [
                    "t",
                    "v"
                ]
            },
            "ChartDataResponse": {
                "type": "object",
                "properties": {
                    "d": {
                        "description": "Chart data",
                        "type": "array",
                        "items": {
                            "$ref": "#/components/schemas/ChartPointResponse"
                        }
                    },
                    "p": {
                        "type": "string",
                        "description": "Chart provider (optional)"
                    }
                },
                "required": [
                    "d",
                    "p"
                ]
            },
            "TokenPriceChangeResponseDto": {
                "type": "object",
                "properties": {
                    "inUSD": {
                        "type": "number"
                    },
                    "inPercent": {
                        "type": "number"
                    }
                },
                "required": [
                    "inUSD",
                    "inPercent"
                ]
            },
            "GetTokenListPriceRequestDto": {
                "type": "object",
                "properties": {
                    "tokenAddresses": {
                        "description": "Addresses of token contracts",
                        "example": [
                            "0x111111111117dC0aa78b770fA6A738034120C302",
                            "0x1f9840a85d5aF5bf1D1762F925BDADdC4201F984"
                        ],
                        "type": "array",
                        "items": {
                            "type": "string"
                        }
                    },
                    "interval": {
                        "type": "string",
                        "enum": [
                            "5m",
                            "10m",
                            "15m",
                            "30m",
                            "50m",
                            "1h",
                            "2h",
                            "3h",
                            "4h",
                            "6h",
                            "12h",
                            "24h",
                            "2d",
                            "3d",
                            "7d",
                            "14d",
                            "15d",
                            "30d",
                            "60d",
                            "90d",
                            "365d",
                            "max"
                        ],
                        "description": "Time interval for price change",
                        "example": "24h"
                    }
                },
                "required": [
                    "tokenAddresses",
                    "interval"
                ]
            },
            "TokenListPriceChangeResponseDto": {
                "type": "object",
                "properties": {
                    "tokenAddress": {
                        "type": "string"
                    },
                    "inUSD": {
                        "type": "number"
                    },
                    "inPercent": {
                        "type": "number"
                    }
                },
                "required": [
                    "tokenAddress",
                    "inUSD",
                    "inPercent"
                ]
            }
        }
    }
