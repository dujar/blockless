        "/v1.0/charts/range/{chain}": {
            "get": {
                "operationId": "ChartsController_getNativeTokenChartByRange",
                "summary": "Returns historical native chain token prices at USD by time range",
                "parameters": [
                    {
                        "name": "chain",
                        "required": true,
                        "in": "path",
                        "description": "Chain id of network",
                        "example": "1",
                        "schema": {
                            "type": "number"
                        }
                    },
                    {
                        "name": "from",
                        "required": true,
                        "in": "query",
                        "description": "unix time at sec",
                        "example": 1622200267,
                        "schema": {
                            "type": "number"
                        }
                    },
                    {
                        "name": "to",
                        "required": true,
                        "in": "query",
                        "description": "unix time at sec",
                        "example": 1622203867,
                        "schema": {
                            "type": "number"
                        }
                    },
                    {
                        "name": "provider",
                        "required": false,
                        "in": "query",
                        "description": "name of chart provider (optional)",
                        "schema": {
                            "enum": [
                                "coinmarketcap",
                                "coingecko",
                                "quantor"
                            ],
                            "type": "string"
                        }
                    },
                    {
                        "name": "from_time",
                        "required": false,
                        "in": "query",
                        "description": "from time (optional)",
                        "example": 1658841720,
                        "schema": {
                            "type": "number"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Return chart data",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/ChartDataResponse"
                                }
                            }
                        }
                    },
                    "500": {
                        "description": "Fatal server error"
                    }
                }
            }
        },
